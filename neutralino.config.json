{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/engine/build/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": ["app.*", "window.*", "init.*"],
  "modes": {
    "window": {
      "title": "Dungeon Busters",
      "width": 1200,
      "height": 800,
      "icon": "/engine/public/favicon.ico",
      "enableInspector": true,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": false
    }
  },
  "cli": {
    "binaryName": "dungeon-buster",
    "resourcesPath": "/engine/build/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/engine/public/neutralino.js",
    "binaryVersion": "4.5.0",
    "clientVersion": "3.4.0",
    "frontendLibrary": {
      "patchFile": "/engine/public/index.html",
      "devUrl": "http://localhost:3000"
    }
  }
}
